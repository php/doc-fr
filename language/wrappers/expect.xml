  &reftitle.options;
  <para>
   <table>
    <title>Résumé du gestionnaire</title>
    <tgroup cols="2">
     <thead>
      <row>
       <entry>Attribut</entry>
       <entry>Supporté</entry>
      </row>
     </thead>
     <tbody>
      <row>
