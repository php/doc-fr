  <refentry id="function.pdf-delete">
   <refnamediv>
    <refname>pdf_delete</refname>
    <refpurpose>Efface un objet PDF</refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
     <methodsynopsis>
      <type>bool</type><methodname>pdf_delete</methodname>
      <methodparam><type>resource</type><parameter>pdfdoc</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>pdf_delete</function> efface l'objet PDF 
     <parameter>pdf_object</parameter>, 
     et libère toutes les ressources.
     &return.success;
    </para>
    <para>
     Voir aussi
     <function>pdf_new</function>.
    </para>
   </refsect1>
  </refentry>

