<refentry xml:id="gmagick.compositeimage" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Gmagick::compositeimage</refname>
  <refpurpose>Assemble 2 images</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>Gmagick</type><methodname>Gmagick::compositeimage</methodname>
   <methodparam><type>Gmagick</type><parameter>source</parameter></methodparam>
   <methodparam><type>int</type><parameter>COMPOSE</parameter></methodparam>
   <methodparam><type>int</type><parameter>x</parameter></methodparam>
   <methodparam><type>int</type><parameter>y</parameter></methodparam>
  </methodsynopsis>
  <para>
   Assemble 2 images à une position spécifique.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>source</parameter></term>
     <listitem>
      <para>
       L'objet <classname>Gmagick</classname> qui contient l'image à assembler.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>COMPOSE</parameter></term>
     <listitem>
      <para>
       L'opérateur d'assemblage.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>x</parameter></term>
     <listitem>
      <para>
       La position de la colonne de l'image assemblée.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>y</parameter></term>
     <listitem>
      <para>
       La position de la ligne de l'image assemblée.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   L'objet <classname>Gmagick</classname> contenant l'assemblage.
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &gmagick.gmagickexception.throw;
  </para>
 </refsect1>

</refentry>

