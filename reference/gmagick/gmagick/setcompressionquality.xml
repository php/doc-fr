<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagick.setcompressionquality">
 <refnamediv>
  <refname>Gmagick::setCompressionQuality</refname>
  <refpurpose>Définit la qualité de compression par défaut de l'objet</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="oop">
   <type>Gmagick</type><methodname>Gmagick::setCompressionQuality</methodname>
   <methodparam>
    <type>int</type><parameter>quality</parameter>
   </methodparam>
  </methodsynopsis>
  <simpara>
   Définit la qualité de compression par défaut de l'objet.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>quality</parameter></term>
    <listitem>
     <simpara>
      La valeur par défaut de GraphicsMagick est 75.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>


 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   L'objet <classname>Gmagick</classname>.
  </simpara>
 </refsect1>
 <refsect1 role="errors">
  &reftitle.errors;
  <simpara>
   &gmagick.gmagickexception.throw;
  </simpara>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <example>
     <title>Exemple avec <function>Gmagick::setCompressionQuality</function></title>
     <programlisting role="php">
     <![CDATA[
<?php
$gm = new Gmagick();
$gm->read("magick:rose");
$gm->setCompressionQuality(2);
?>
]]>
     </programlisting>
   </example>
 </refsect1>

</refentry>
