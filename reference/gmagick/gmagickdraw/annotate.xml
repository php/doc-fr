<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagickdraw.annotate">
 <refnamediv>
  <refname>GmagickDraw::annotate</refname>
  <refpurpose>Dessine un texte sur l'image</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>GmagickDraw</type><methodname>GmagickDraw::annotate</methodname>
   <methodparam><type>float</type><parameter>x</parameter></methodparam>
   <methodparam><type>float</type><parameter>y</parameter></methodparam>
   <methodparam><type>string</type><parameter>text</parameter></methodparam>
  </methodsynopsis>
  <simpara>
   Dessine un texte sur l'image.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>x</parameter></term>
    <listitem>
     <simpara>
      Coordonnée en X de la gauche du texte.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>y</parameter></term>
    <listitem>
     <simpara>
      Coordonnée en Y de la base du texte.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>text</parameter></term>
    <listitem>
     <simpara>
      Le texte à dessiner.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   L'objet <classname>GmagickDraw</classname>.
  </simpara>
 </refsect1>

</refentry>
