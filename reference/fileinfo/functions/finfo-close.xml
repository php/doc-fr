<refentry xml:id="function.finfo-close" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>finfo_close</refname>
  <refpurpose>Ferme une ressource fileinfo</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="procedural">
   <type>bool</type><methodname>finfo_close</methodname>
   <methodparam><type>resource</type><parameter>finfo</parameter></methodparam>
  </methodsynopsis>

  <para>
   Cette fonction ferme la ressource ouverte par
   <function>finfo_open</function>.
  </para>

 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>finfo</parameter></term>
     <listitem>
      <para>
       Ressource Fileinfo retourn√©e par <function>finfo_open</function>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

</refentry>

