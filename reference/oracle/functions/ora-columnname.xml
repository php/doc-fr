<?xml version="1.0" encoding="iso-8859-1"?>
<!-- splitted from ./fr/functions/oracle.xml, last change in rev 1.2 -->
<!-- last change to 'ora-columnname' in en/ tree in rev 1.2 -->
  <refentry id="function.ora-columnname">
   <refnamediv>
    <refname>ora_columnname</refname>
    <refpurpose>Retourne le nom de la colonne de r&eacute;sultat Oracle</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>string</type><methodname>ora_columnname</methodname>
      <methodparam><type>resource</type><parameter>cursor</parameter></methodparam>
      <methodparam><type>int</type><parameter>column</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>ora_columnname</function> retourne le nom du champ
     <parameter>column</parameter> du pointeur <parameter>cursor</parameter>.
     Le nom retourn&eacute; sera en majuscules.
    </para>
   </refsect1>
  </refentry>

