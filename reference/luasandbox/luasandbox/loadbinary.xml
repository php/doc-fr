<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="luasandbox.loadbinary">
 <refnamediv>
  <refname>LuaSandbox::loadBinary</refname>
  <refpurpose>Charge un morceau binaire précompilé dans l'environnement Lua</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>LuaSandboxFunction</type><methodname>LuaSandbox::loadBinary</methodname>
   <methodparam><type>string</type><parameter>code</parameter></methodparam>
   <methodparam choice="opt"><type>string</type><parameter>chunkName</parameter><initializer>''</initializer></methodparam>
  </methodsynopsis>
  <simpara>
   Charge les données générées par <methodname>LuaSandboxFunction::dump</methodname>.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>code</parameter></term>
    <listitem>
     <simpara>
      Les données de <methodname>LuaSandboxFunction::dump</methodname>.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>chunkName</parameter></term>
    <listitem>
     <simpara>
      Le nom du morceau pour la fonction chargée.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   Renvoie une <classname>LuaSandboxFunction</classname>.
  </simpara>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <simplelist>
   <member><methodname>LuaSandbox::loadString</methodname></member>
  </simplelist>
 </refsect1>

</refentry>
