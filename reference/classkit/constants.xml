<appendix xml:id="classkit.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <para>
  <variablelist>
   <varlistentry xml:id="constant.classkit-acc-private">
    <term>
     <constant>CLASSKIT_ACC_PRIVATE</constant> 
     (&integer;)
    </term>
    <listitem>
     <simpara>
      Rend la méthode privée ("<literal>private</literal>")
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="constant.classkit-acc-protected">
    <term>
     <constant>CLASSKIT_ACC_PROTECTED</constant> 
     (&integer;)
    </term>
    <listitem>
     <simpara>
      Rend la méthode protégée ("<literal>protected</literal>")
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="constant.classkit-acc-public">
    <term>
     <constant>CLASSKIT_ACC_PUBLIC</constant> 
     (&integer;)
    </term>
    <listitem>
     <simpara>
      Rend la méthode publique ("<literal>public</literal>")
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </para>
</appendix>

