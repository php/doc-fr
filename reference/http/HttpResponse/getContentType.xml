<refentry xml:id="function.httpresponse-getcontenttype" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpResponse::getContentType</refname>
  <refpurpose>Récupère l'en-tête <literal>Content-Type</literal></refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>static</modifier>
   <type>string</type><methodname>HttpResponse::getContentType</methodname>
   <void/>
  </methodsynopsis>

  <para>
   Récupère l'en-tête <literal>Content-Type</literal> courant.
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne l'en-tête <literal>Content-Type</literal> courant sous la forme d'une
   &string;.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>HttpResponse::getContentDisposition</methodname></member>
    <member><methodname>HttpResponse::setContentDisposition</methodname></member>
    <member><methodname>HttpResponse::setContentType</methodname></member>
    <member><methodname>HttpResponse::guessContentType</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

