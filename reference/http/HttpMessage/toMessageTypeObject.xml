<refentry xml:id="httpmessage.tomessagetypeobject" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpMessage::toMessageTypeObject</refname>
  <refpurpose>Crée un objet HTTP en fonction du type de message</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
   <type>HttpRequest|HttpResponse</type><methodname>HttpMessage::toMessageTypeObject</methodname>
   <void/>
  </methodsynopsis>

  <para>
   Crée un objet en fonction du type de message.
  </para>

 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne soit un objet <classname>HttpRequest</classname>,
   soit un objet <classname>HttpResponse</classname>
   en cas de succès, ou &null; si une erreur survient.
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   Lance une exception <classname>HttpRuntimeException</classname>,
   <classname>HttpMessageTypeException</classname>
   ou <classname>HttpHeaderException</classname>.
  </para>
 </refsect1>
</refentry>

