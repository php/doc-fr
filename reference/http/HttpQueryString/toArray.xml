<refentry xml:id="function.httpquerystring-toarray" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpQueryString::toArray</refname>
  <refpurpose>Récupère la requête sous la forme d'un tableau</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
   <type>array</type><methodname>HttpQueryString::toArray</methodname>
   <void/>
  </methodsynopsis>

  <para>
   Récupère la requête sous la forme d'un tableau associatif.
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne un tableau représentant la requête.
  </para>
 </refsect1>
</refentry>

