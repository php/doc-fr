<refentry xml:id="function.httprequestpool-send" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpRequestPool::send</refname>
  <refpurpose>Envoie toutes les requêtes</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>HttpRequestPool::send</methodname>
   <void/>
  </methodsynopsis>

  <para>
   Envoie tous les objets HttpRequest attachés en parallèle.
  </para>

 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   Lance une exception <classname>HttpRequestPoolException</classname>
   (<classname>HttpSocketException</classname>,
   <classname>HttpRequestException</classname>,
   <classname>HttpMalformedHeaderException</classname>).
  </para>
 </refsect1>
</refentry>

