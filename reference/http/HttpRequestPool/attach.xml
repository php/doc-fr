<refentry xml:id="httprequestpool.attach" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpRequestPool::attach</refname>
  <refpurpose>Attache un objet HttpRequest</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
   <type>bool</type><methodname>HttpRequestPool::attach</methodname>
   <methodparam><type>HttpRequest</type><parameter>request</parameter></methodparam>
  </methodsynopsis>

  <para>
   Attache un objet <classname>HttpRequest</classname>
   à ce <classname>HttpRequestPool</classname>.
  </para>
  <warning>
   <para>
    Définissez toutes les options avant d'effectuer l'attachement !
   </para>
  </warning>

 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>request</parameter></term>
     <listitem>
      <para>
       Un objet <classname>HttpRequest</classname> qui n'est pas
       déjà attaché à un objet <classname>HttpRequestPool</classname>
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   Lance une exception <classname>HttpInvalidParamException</classname>,
   <classname>HttpRequestException</classname>,
   <classname>HttpRequestPoolException</classname>,
   <classname>HttpEncodingException</classname>.
  </para>
 </refsect1>
</refentry>

