<refentry xml:id="function.httprequest-setmethod" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpRequest::setMethod</refname>
  <refpurpose>Définit la méthode</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
   <type>bool</type><methodname>HttpRequest::setMethod</methodname>
   <methodparam><type>int</type><parameter>request_method</parameter></methodparam>
  </methodsynopsis>

  <para>
   Définit la méthode de la requête.
  </para>

 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>request_method</parameter></term>
     <listitem>
      <para>
       la méthode de la requête à utiliser.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
</refentry>

