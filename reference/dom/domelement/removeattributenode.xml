<refentry xml:id='domelement.removeattributenode' xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DOMElement::removeAttributeNode</refname>
  <refpurpose>Efface un attribut</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>DOMElement::removeAttributeNode</methodname>
   <methodparam><type>DOMAttr</type><parameter>oldnode</parameter></methodparam>
  </methodsynopsis>
  <para>
   Efface l'attribut <parameter>oldnode</parameter> de l'élément.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>oldnode</parameter></term>
     <listitem>
      <para>
       Le noeud de l'attribut.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   <variablelist>
    <varlistentry>
     <term><constant>DOM_NO_MODIFICATION_ALLOWED_ERR</constant></term>
     <listitem>
      <para>
       Lancé si le noeud est en lecture seule.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><constant>DOM_NOT_FOUND_ERROR</constant></term>
     <listitem>
      <para>
       Lancé si <parameter>oldnode</parameter> n'est pas un attribut
       de l'élément.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>DOMElement::hasAttribute</methodname></member>
    <member><methodname>DOMElement::getAttributeNode</methodname></member>
    <member><methodname>DOMElement::setAttributeNode</methodname></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

