<refentry xml:id='domtext.iswhitespaceinelementcontent' xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DOMText::isWhitespaceInElementContent</refname>
  <refpurpose>
   Indique si ce noeud de texte contient des espaces blancs
  </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>DOMText::isWhitespaceInElementContent</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Indique si un noeud de texte contient des espaces. Cette
   détermination est faite durant le chargement du document.
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
</refentry>

