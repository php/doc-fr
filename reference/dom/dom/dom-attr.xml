  <section xml:id="dom-attr.props">
   &reftitle.properties;
   <variablelist>
    <varlistentry xml:id="dom-attr.props.namespaceuri">
     <term><varname>namespaceURI</varname></term>
     <listitem>
      <simpara>L'<acronym>URI</acronym> du namespace de l'attribut.</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.prefix">
     <term><varname>prefix</varname></term>
     <listitem>
      <simpara>Le préfixe du namespace de l'attribut.</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.localname">
     <term><varname>localName</varname></term>
     <listitem>
      <simpara>Le nom local de l'attribut.</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.name">
     <term><varname>name</varname></term>
     <listitem>
      <simpara>Le nom qualifié de l'attribut.</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.value">
     <term><varname>value</varname></term>
     <listitem>
      <simpara>La valeur de l'attribut.</simpara>
      <note>
       <simpara>
        Contrairement à la propriété équivalente dans <classname>DOMAttr</classname>,
        ceci ne substitue pas les entités.
       </simpara>
      </note>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.ownerelement">
     <term><varname>ownerElement</varname></term>
     <listitem>
      <simpara>L'élément qui contient l'attribut ou &null;.</simpara>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="dom-attr.props.specified">
     <term><varname>specified</varname></term>
     <listitem>
      <simpara>Option héritée, toujours &true;.</simpara>
     </listitem>
    </varlistentry>
   </variablelist>
  </section>

