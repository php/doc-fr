<refentry xml:id="function.runkit-return-value-used" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>runkit_return_value_used</refname>
  <refpurpose>Détermine si la valeur de retour des fonctions courantes sera utilisée</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>runkit_return_value_used</methodname>
   <void/>
  </methodsynopsis>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne &true; si la valeur de retour d'une fonction est utilisée par la
   portée appelante, &false; autrement.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>runkit_return_value_used</function></title>
    <programlisting role="php">
<![CDATA[
<?php
function foo() {
  var_dump(runkit_return_value_used());
}

foo();
$f = foo();
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
bool(false)
bool(true)
]]>
    </screen>
   </example>
  </para>
 </refsect1>

</refentry>

