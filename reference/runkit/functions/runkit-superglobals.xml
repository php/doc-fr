<refentry xml:id="function.runkit-superglobals" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>runkit_superglobals</refname>
  <refpurpose>
   Retourne un tableau indexé numériquement des variables superglobales enregistrées
  </refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>runkit_superglobals</methodname>
   <void/>
  </methodsynopsis>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne un tableau indexé numériquement des variables superglobales
   enregistrée actuellement.
   C'est-à-dire _GET, _POST, _REQUEST, _COOKIE, _SESSION, _SERVER, _ENV, _FILES
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><link linkend="language.variables.scope">Portées des Variables</link></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

