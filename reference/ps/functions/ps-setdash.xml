<refentry xml:id="function.ps-setdash" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ps_setdash</refname>
  <refpurpose>Fixe l'apparence d'une ligne pointillée</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ps_setdash</methodname>
   <methodparam><type>resource</type><parameter>psdoc</parameter></methodparam>
   <methodparam><type>float</type><parameter>on</parameter></methodparam>
   <methodparam><type>float</type><parameter>off</parameter></methodparam>
  </methodsynopsis>
  <para>
   Fixe la longueur des portions de noir et blanc d'une ligne pointillée. 
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>psdoc</parameter></term>
     <listitem>
      <para>
       Identifiant d'un fichier postscript retourné par
       <function>ps_new</function>.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>on</parameter></term>
     <listitem>
      <para>
       La grandeur des pointillés.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>off</parameter></term>
     <listitem>
      <para>
       La grandeur des espaces entres les pointillés.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>ps_setpolydash</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

