      <classname>RecursiveArrayIterator</classname>
     </ooclass>

     <ooclass>
      <modifier>extends</modifier>
      <classname>ArrayIterator</classname>
     </ooclass>

     <oointerface>
      <interfacename>RecursiveIterator</interfacename>
