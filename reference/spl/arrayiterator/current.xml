<refentry xml:id="arrayiterator.current" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ArrayIterator::current</refname>
  <refpurpose>Retourne l'entrée courante du tableau</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="ArrayIterator">
   <modifier>public</modifier> <type>mixed</type><methodname>ArrayIterator::current</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retourne l'entrée courante du tableau.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   L'entrée courante du tableau.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>ArrayIterator::current</function></title>
    <programlisting role="php">
<![CDATA[
<?php
$array = array('1' => 'one',
               '2' => 'two',
               '3' => 'three');

$arrayobject = new ArrayObject($array);

for($iterator = $arrayobject->getIterator();
    $iterator->valid();
    $iterator->next()) {

    echo $iterator->key() . ' => ' . $iterator->current() . "\n";
}
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
1 => one
2 => two
3 => three
]]>
    </screen>
   </example>
  </para>
 </refsect1>
</refentry>

