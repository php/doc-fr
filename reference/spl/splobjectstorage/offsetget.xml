<refentry xml:id="splobjectstorage.offsetget" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>SplObjectStorage::offsetGet</refname>
  <refpurpose>Retourne les données associées à un &object;</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="SplObjectStorage">
   <modifier>public</modifier> <type>mixed</type><methodname>SplObjectStorage::offsetGet</methodname>
   <methodparam><type>object</type><parameter>object</parameter></methodparam>
  </methodsynopsis>
  <para>
   Retourne les données associées à un &object; dans le stockage.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>object</parameter></term>
     <listitem>
      <para>
       L'&object; à rechercher.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Les données précédemment associées avec l'&object;.
  </para>
 </refsect1>
 
 <refsect1 role="errors"><!-- {{{ -->
  &reftitle.errors;
  <para>
   Lance une exception <classname>UnexpectedValueException</classname>
   lorsque <parameter>object</parameter> n'a pû être trouvé.
  </para>
 </refsect1><!-- }}} -->

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>SplObjectStorage::offsetGet</function></title>
    <programlisting role="php">
<![CDATA[
<?php
$s = new SplObjectStorage;

$o1 = new StdClass;
$o2 = new StdClass;

$s[$o1] = "hello";
$s->attach($o2);


var_dump($s->offsetGet($o1)); // Similaire à $s[$o1]
var_dump($s->offsetGet($o2)); // Similaire à $s[$o2]
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
string(5) "hello"
NULL
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>SplObjectStorage::offsetSet</methodname></member>
    <member><methodname>SplObjectStorage::offsetExists</methodname></member>
    <member><methodname>SplObjectStorage::offsetUnset</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

