<refentry xml:id="function.ArrayIterator-key" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ArrayIterator::key</refname>
  <refpurpose>
   Retourne la clé courante du tableau
  </refpurpose>
 </refnamediv>
 <refsect1>
  &reftitle.description;
  <methodsynopsis>
   <type>mixed</type><methodname>ArrayIterator::key</methodname>
   <void/>
  </methodsynopsis>
  <para>
   <function>ArrayIterator::key</function> retourne la clé courante du tableau.
  </para>
  <para>
   <example>
    <title>Exemple avec <function>ArrayIterator::key</function></title>
    <programlisting role="php">
     <![CDATA[
<?php
$array = array('key' => 'value');

$arrayobject = new ArrayObject($array);
$iterator = $arrayobject->getIterator();

echo $iterator->key(); //key
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
</refentry>

