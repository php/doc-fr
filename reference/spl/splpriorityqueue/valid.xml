<refentry xml:id="splpriorityqueue.valid" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SplPriorityQueue::valid</refname>
  <refpurpose>Vérifie si la file d'attente contient encore des nœuds</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="SplPriorityQueue">
   <modifier>public</modifier> <type>bool</type><methodname>SplPriorityQueue::valid</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Vérifie si la file d'attente contient encore des nœuds.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne &true; si la file d'attente contient encore des nœuds, &false; sinon.
  </para>
 </refsect1>

</refentry>
