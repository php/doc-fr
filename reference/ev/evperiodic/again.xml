 <refnamediv>
  <refname>EvPeriodic::again</refname>
  <refpurpose>Arrête et re-démarre le watcher périodique</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
