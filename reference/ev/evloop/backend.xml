 <refnamediv>
  <refname>EvLoop::backend</refname>
  <refpurpose>Retourne un entier décrivant le backend utilisé par libev</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
   <type>int</type>
   <methodname>EvLoop::backend</methodname>
   <void />
  </methodsynopsis>
  <para>
   Identique à la méthode <methodname>Ev::backend</methodname>,
   mais pour l'instance de la boucle.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>
