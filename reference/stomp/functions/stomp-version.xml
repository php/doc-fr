<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="function.stomp-version">
 <refnamediv>
  <refname>stomp_version</refname>
  <refpurpose>Récupère la version de l'extension Stomp</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>stomp_version</methodname>
   <void/>
  </methodsynopsis>
  <simpara>
   Retourne une chaîne contenant la version de l'extension Stomp.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   Version de l'extension Stomp.
  </simpara>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <example>
   <title>Exemple avec <function>stomp_version</function></title>
   <programlisting role="php">
<![CDATA[
<?php

var_dump(stomp_version());

?>
]]>
   </programlisting>
   &example.outputs.similar;
   <screen>
<![CDATA[
string(5) "0.2.0"
]]>
   </screen>
  </example>
 </refsect1>


</refentry>
