<refentry xml:id="function.stomp-version" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>stomp_version</refname>
  <refpurpose>Récupère la version de l'extension Stomp</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>stomp_version</methodname>
   <void />
  </methodsynopsis>
  <para>
   Retourne une chaîne contenant la version de l'extension Stomp.
  </para>
 </refsect1>
 
 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Version de l'extension Stomp.
  </para>
 </refsect1>
 
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>stomp_version</function></title>
    <programlisting role="php">
<![CDATA[
<?php

var_dump(stomp_version());

?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
string(5) "0.2.0"
]]>
    </screen>
   </example>
  </para>
 </refsect1>
 
 
</refentry>

