<refentry xml:id="reflectionfunctionabstract.returnsreference" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionFunctionAbstract::returnsReference</refname>
  <refpurpose>Vérifie si la fonction retourne une référence</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>ReflectionFunctionAbstract::returnsReference</methodname>
   <void />
  </methodsynopsis>
  <para>
   Vérifie si la fonction retourne une référence.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &true; si la fonction retourne une référence, &false; sinon.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionFunctionAbstract::isClosure</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

