<refentry xml:id="function.svn-fs-delete" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>svn_fs_delete</refname>
  <refpurpose>Supprime un fichier ou un dossier</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>svn_fs_delete</methodname>
   <methodparam><type>resource</type><parameter>root</parameter></methodparam>
   <methodparam><type>string</type><parameter>path</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Supprime un fichier ou un dossier ; retourne &true; en cas de succès, &false;
   sinon.
  </para>
 </refsect1>

 <refsect1 role="notes">
  &reftitle.notes;
  &warn.experimental.func;
 </refsect1>

</refentry>

