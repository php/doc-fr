<appendix xml:id="filter.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <variablelist>
  <varlistentry xml:id="constant.input-post">
   <term>
    <constant>INPUT_POST</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     variables <link linkend="reserved.variables.post">POST</link>.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.input-get">
   <term>
    <constant>INPUT_GET</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     variables <link linkend="reserved.variables.get">GET</link>.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.input-cookie">
   <term>
    <constant>INPUT_COOKIE</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     variables <link linkend="reserved.variables.cookies">COOKIE</link>.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.input-env">
   <term>
    <constant>INPUT_ENV</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     variables <link linkend="reserved.variables.environment">ENV</link>.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.input-server">
   <term>
    <constant>INPUT_SERVER</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     variables <link linkend="reserved.variables.server">SERVER</link>.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.input-session">
   <term>
    <constant>INPUT_SESSION</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     variables <link linkend="reserved.variables.session">SESSION</link>.
     (pas encore implémenté)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.input-request">
   <term>
    <constant>INPUT_REQUEST</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     variables <link linkend="reserved.variables.request">REQUEST</link>.
    (pas encore implémenté)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-none">
   <term>
    <constant>FILTER_FLAG_NONE</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Aucun drapeau.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-require-scalar">
   <term>
    <constant>FILTER_REQUIRE_SCALAR</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Drapeau utilisé pour requérir un scalaire en entrée.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-require-array">
   <term>
    <constant>FILTER_REQUIRE_ARRAY</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Requière un tableau en entrée.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-force-array">
   <term>
    <constant>FILTER_FORCE_ARRAY</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Retourne toujours un tableau.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-null-on-failure">
   <term>
    <constant>FILTER_NULL_ON_FAILURE</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Utilisation de &null; au lieu de &false; si une erreur survient.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-int">
   <term>
    <constant>FILTER_VALIDATE_INT</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "int".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-boolean">
   <term>
    <constant>FILTER_VALIDATE_BOOLEAN</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "boolean".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-float">
   <term>
    <constant>FILTER_VALIDATE_FLOAT</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "float".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-regexp">
   <term>
    <constant>FILTER_VALIDATE_REGEXP</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "validate_regexp".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-url">
   <term>
    <constant>FILTER_VALIDATE_URL</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "validate_url".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-domain">
   <term>
    <constant>FILTER_VALIDATE_DOMAIN</constant>
     (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     ID du filtre "validate_domain".
     (Disponible depuis PHP 7.0.0)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-email">
   <term>
    <constant>FILTER_VALIDATE_EMAIL</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "validate_email".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-ip">
   <term>
    <constant>FILTER_VALIDATE_IP</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "validate_ip".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-validate-mac">
   <term>
    <constant>FILTER_VALIDATE_MAC</constant>
     (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     ID du filtre "validate_mac_address".
     (Disponible depuis PHP 5.5.0)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-default">
   <term>
    <constant>FILTER_DEFAULT</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre par défaut ("unsafe_raw").
     Ceci est équivalent à la constante 
     <constant>FILTER_UNSAFE_RAW</constant>.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-unsafe-raw">
   <term>
    <constant>FILTER_UNSAFE_RAW</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "unsafe_raw".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-string">
   <term>
    <constant>FILTER_SANITIZE_STRING</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "string".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-stripped">
   <term>
    <constant>FILTER_SANITIZE_STRIPPED</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "stripped".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-encoded">
   <term>
    <constant>FILTER_SANITIZE_ENCODED</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "encoded".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-special-chars">
   <term>
    <constant>FILTER_SANITIZE_SPECIAL_CHARS</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "special_chars".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-email">
   <term>
    <constant>FILTER_SANITIZE_EMAIL</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "email".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-url">
   <term>
    <constant>FILTER_SANITIZE_URL</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "url".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-number-int">
   <term>
    <constant>FILTER_SANITIZE_NUMBER_INT</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "number_int".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-number-float">
   <term>
    <constant>FILTER_SANITIZE_NUMBER_FLOAT</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "number_float".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-magic-quotes">
   <term>
    <constant>FILTER_SANITIZE_MAGIC_QUOTES</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre "magic_quotes".
     (Obsolète depuis PHP 7.3.0, utilisez plutôt FILTER_SANITIZE_ADD_SLASHES.)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-sanitize-add-slashes">
   <term>
    <constant>FILTER_SANITIZE_ADD_SLASHES</constant>
     (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     ID du filtre "add_slashes".
     (Disponible depuis PHP 7.3.0)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-callback">
   <term>
    <constant>FILTER_CALLBACK</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     ID du filtre de rappel.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-allow-octal">
   <term>
    <constant>FILTER_FLAG_ALLOW_OCTAL</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Alloue une notation octale (<literal>0[0-7]+</literal>) dans le filtre "int".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-allow-hex">
   <term>
    <constant>FILTER_FLAG_ALLOW_HEX</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Alloue une notation hexadécimale (<literal>0x[0-9a-fA-F]+</literal>) dans le filtre "int".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-strip-low">
   <term>
    <constant>FILTER_FLAG_STRIP_LOW</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Supprime les caractères dont les valeurs ASCII sont inférieures à 32.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-strip-high">
   <term>
    <constant>FILTER_FLAG_STRIP_HIGH</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Supprime les caractères dont les valeurs ASCII sont supérieures à 127.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-strip-backtick">
   <term>
    <constant>FILTER_FLAG_STRIP_BACKTICK</constant>
     (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Supprime les accents grâves.
     (Disponible depuis PHP 5.3.2)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-encode-low">
   <term>
    <constant>FILTER_FLAG_ENCODE_LOW</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Encode les caractères dont les valeurs ASCII sont inférieures à 32.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-encode-high">
   <term>
    <constant>FILTER_FLAG_ENCODE_HIGH</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Encode les caractères dont les valeurs ASCII sont supérieures à 127.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-encode-amp">
   <term>
    <constant>FILTER_FLAG_ENCODE_AMP</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Encode <literal>&amp;</literal>.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-no-encode-quotes">
   <term>
    <constant>FILTER_FLAG_NO_ENCODE_QUOTES</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     N'encode pas <literal>'</literal>, ni <literal>"</literal>.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-empty-string-null">
   <term>
    <constant>FILTER_FLAG_EMPTY_STRING_NULL</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     (Non utilisé actuellement.)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-allow-fraction">
   <term>
    <constant>FILTER_FLAG_ALLOW_FRACTION</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Alloue une partie fractionnelle dans le filtre "number_float".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-allow-thousand">
   <term>
    <constant>FILTER_FLAG_ALLOW_THOUSAND</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Alloue le séparateur des milliers (<literal>,</literal>) dans le filtre "number_float".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-allow-scientific">
   <term>
    <constant>FILTER_FLAG_ALLOW_SCIENTIFIC</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Alloue une notation scientifique (<literal>e</literal>, <literal>E</literal>) dans
     le filtre "number_float".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-path-required">
   <term>
    <constant>FILTER_FLAG_PATH_REQUIRED</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Chemin requis dans le filtre "validate_url".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-query-required">
   <term>
    <constant>FILTER_FLAG_QUERY_REQUIRED</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Requête requise dans le filtre "validate_url".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-scheme-required">
   <term>
    <constant>FILTER_FLAG_SCHEME_REQUIRED</constant>
     (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Requière un schéma dans le filtre "validate_url".
     (Obsolète depuis PHP 7.3 car déjà présent dans le filtre)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-host-required">
   <term>
    <constant>FILTER_FLAG_HOST_REQUIRED</constant>
     (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Requière un hôte dans le filtre "validate_url".
     (Obsolète depuis PHP 7.3 car déjà présent dans le filtre)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-hostname">
   <term>
    <constant>FILTER_FLAG_HOSTNAME</constant>
     (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Demande à ce que les noms d'hôtes commencent avec un caractère
     alphanumérique et contiennent que les caractères alphanumériques ou
     des traits d'union
     (Disponible depuis PHP 7.0.0)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-ipv4">
   <term>
    <constant>FILTER_FLAG_IPV4</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     N'autorise que les adresses IPv4 dans le filtre "validate_ip".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-ipv6">
   <term>
    <constant>FILTER_FLAG_IPV6</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     N'autorise que les adresses IPv6 dans le filtre "validate_ip".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-no-res-range">
   <term>
    <constant>FILTER_FLAG_NO_RES_RANGE</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     N'autorise pas les adresses réservées dans le filtre "validate_ip".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-no-priv-range">
   <term>
    <constant>FILTER_FLAG_NO_PRIV_RANGE</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     N'autorise pas les adresses privées dans le filtre "validate_ip".
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.filter-flag-email-unicode">
   <term>
    <constant>FILTER_FLAG_EMAIL_UNICODE</constant>
     (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Accepte des charactères Unicode dans la partie locale du filtre "validate_email".
     (Disponible depuis PHP 7.1.0)
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>
</appendix>

