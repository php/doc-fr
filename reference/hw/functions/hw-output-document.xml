<!-- splitted from ./fr/functions/hw.xml, last change in rev 1.11 -->
<!-- last change to 'hw-outputdocument' in en/ tree in rev 1.1 -->
  <refentry id="function.hw-output-document">
   <refnamediv>
    <refname>hw_output_document</refname>
    <refpurpose>Affiche hw_document</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>int</type><methodname>hw_output_document</methodname>
      <methodparam><type>int</type><parameter>hw_document</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>hw_output_document</function> affiche
     <parameter>hw_document</parameter> sans la balise BODY.
    </para>
    <para>
     Pour la compatibilité ascendente, <function>hw_outputdocument</function>
     est aussi accepté, mais il est déconseillé.
    </para>      
   </refsect1>
  </refentry>

