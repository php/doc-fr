<!-- splitted from ./fr/functions/hw.xml, last change in rev 1.11 -->
<!-- last change to 'hw-documentsize' in en/ tree in rev 1.1 -->
  <refentry id="function.hw-document-size">
   <refnamediv>
    <refname>hw_Document_Size</refname>
    <refpurpose>Taille d'un document.</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>int</type><methodname>hw_document_size</methodname>
      <methodparam><type>int</type><parameter>hw_document</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>hw_document_size</function> retourne la taille du document
     en octets.
    </para>
    <para>
     Voir aussi
     <function>hw_document_bodytag</function> et
     <function>hw_document_attributes</function>.
    </para>
    <para>
     Pour la compatibilit&eacute; ascendente, <function>hw_documentsize</function>
     est aussi accept&eacute;, mais il est d&eacute;conseill&eacute;.
    </para>    
   </refsect1>
  </refentry>

