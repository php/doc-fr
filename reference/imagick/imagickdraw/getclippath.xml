<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagickdraw-getclippath">
 <refnamediv>
  <refname>ImagickDraw::getClipPath</refname>
  <refpurpose>Retourne l'identifiant du chemin courant</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>ImagickDraw::getClipPath</methodname>
   <void/>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Retourne l'identifiant du chemin courant.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne une chaîne, ou &false; si aucun chemin n'existe.
  </para>
 </refsect1>

</refentry>

