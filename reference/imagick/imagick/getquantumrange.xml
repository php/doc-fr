<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.getquantumrange">
 <refnamediv>
  <refname>Imagick::getQuantumRange</refname>
  <refpurpose>Retourne l'intervalle quantique de Imagick</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <modifier>static</modifier> <type>array</type><methodname>Imagick::getQuantumRange</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retourne l'intervalle quantique de l'instance Imagick.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne un tableau associatif contenant l'intervalle quantique
   sous la forme d'un &integer; (<literal>"quantumRangeLong"</literal>)
   et sous la forme d'une &string;
   (<literal>"quantumRangeString"</literal>).
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &imagick.imagick.throws;
  </para>
 </refsect1>

</refentry>

