<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.getimagecolors">
 <refnamediv>
  <refname>Imagick::getImageColors</refname>
  <refpurpose>Lit le nombre de couleurs uniques de l'image</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>Imagick::getImageColors</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Lit le nombre de couleurs uniques de l'image.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne un <type>integer</type>, le nombre de couleur unique dans l'image.
  </para>
 </refsect1>

</refentry>

