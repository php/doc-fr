<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.imagick-getpixelregioniterator">
 <refnamediv>
  <refname>Imagick::getPixelRegionIterator</refname>
  <refpurpose>Lit un objet ImagickPixelIterator pour une section d'image</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>ImagickPixelIterator</type><methodname>Imagick::getPixelRegionIterator</methodname>
   <methodparam><type>int</type><parameter>x</parameter></methodparam>
   <methodparam><type>int</type><parameter>y</parameter></methodparam>
   <methodparam><type>int</type><parameter>columns</parameter></methodparam>
   <methodparam><type>int</type><parameter>rows</parameter></methodparam>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
   Lit un objet ImagickPixelIterator pour une section d'image.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>x</parameter></term>
     <listitem>
      <para>
       L'abscisse de la région.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>y</parameter></term>
     <listitem>
      <para>
       L'ordonnée de la région.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>columns</parameter></term>
     <listitem>
      <para>
       La largeur de la région.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>rows</parameter></term>
     <listitem>
      <para>
       La hauteur de la région.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne un objet ImagickPixelIterator pour une section d'image.
   &imagick.imagickexception.throw;
  </para>
 </refsect1>

</refentry>

