<refentry xml:id="function.judy-type" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>judy_type</refname>
  <refpurpose>Retourne le type de tableau <classname>Judy</classname></refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>integer</type><methodname>judy_type</methodname>
   <methodparam><type>Judy</type><parameter>array</parameter></methodparam>
  </methodsynopsis>
  <para>
   <function>judy_type</function> Retourne un entier correspondant au 
   <link linkend="judy.types">type</link> Judy pour le <parameter>tableau</parameter>
   passé en paramétre.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>array</parameter></term>
    <listitem>
     <para>
      Le tableau <classname>Judy</classname> à tester.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
    Retourne un entier correspondant au <link linkend="judy.types">type</link> Judy.
  </para>
 </refsect1>


</refentry>

