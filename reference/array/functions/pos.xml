<!-- last change to 'pos' in en/ tree in rev 1.2 -->
 <refentry id="function.pos">
  <refnamediv>
   <refname>pos</refname>
   <refpurpose>Retourne l'élément courant d'un tableau </refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>mixed</type><methodname>pos</methodname>
     <methodparam><type>array</type><parameter>array</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>pos</function> est une fonction alias de <function>current</function>.
   </para>
   <para>
    Voir aussi
    <function>end</function>,
    <function>next</function>,
    <function>prev</function> et 
    <function>reset</function>.
   </para>
  </refsect1>
 </refentry>

