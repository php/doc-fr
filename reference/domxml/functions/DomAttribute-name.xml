<refentry xml:id="function.domattribute-name" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DomAttribute->name</refname>
  <refpurpose>
   Retourne le nom de l'attribut
  </refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <classsynopsis>
   <ooclass><classname>DomAttribute</classname></ooclass>
   <methodsynopsis>
    <type>string</type><methodname>name</methodname>
    <void/>
   </methodsynopsis>
  </classsynopsis>
  <para>
   Récupère le nom de l'attribut.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne le nom de l'attribut.
  </para>
 </refsect1>

 <refsect1 role="migration">
  <title>Migration vers PHP 5</title>
  <para>
   Utilisez la propriété <property>name</property> de 
   <classname>DOMAttr</classname>.
  </para>
 </refsect1>
 
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><xref linkend="function.domattribute-value" /> pour un exemple</member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

