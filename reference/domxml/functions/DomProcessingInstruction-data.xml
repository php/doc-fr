<refentry xml:id="function.domprocessinginstruction-data" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DomProcessingInstruction->data</refname>
  <refpurpose>
   Retourne les données du noeud l'instruction en cours
  </refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <classsynopsis>
   <ooclass><classname>DomProcessingInstruction</classname></ooclass>
   <methodsynopsis>
    <type>string</type><methodname>data</methodname>
    <void/>
   </methodsynopsis>
  </classsynopsis>
  <para>
   Cette méthode retourne les données du noeud de l'instruction en cours.
  </para>
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne les données de l'instruction en cours.
  </para>
 </refsect1>
 
 <refsect1 role="migration">
  <title>Migration vers PHP 5</title>
  <para>
   Utilisez la propriété <property>data</property> de
   <classname>DOMProcessingInstruction</classname>.
  </para>
 </refsect1>
</refentry>

