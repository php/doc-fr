<refentry xml:id="xmlreader.readouterxml" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>XMLReader::readOuterXML</refname>
  <refpurpose>Lit le code XML du noeud courant, y compris lui-même</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>XMLReader::readOuterXML</methodname>
   <void />
  </methodsynopsis>
  <para>
   Lit le code XML du noeud courant, y compris lui-même.
  </para>
 </refsect1>
 
 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>
 
 <refsect1 role="returnvalues">
