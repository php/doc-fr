<refentry xml:id="quickhashinthash.getsize" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>QuickHashIntHash::getSize</refname>
  <refpurpose>Retourne le nombre d'éléments dans le hash</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>integer</type><methodname>QuickHashIntHash::getSize</methodname><void/>
  </methodsynopsis>
  <para>
   Retourne le nombre d'éléments dans le hash.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Le nombre d'éléments dans le hash
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple <function>QuickHashIntHash::getSize</function></title>
    <programlisting role="php">
<![CDATA[
<?php
$hash = new QuickHashIntHash( 8 );
var_dump( $hash->add( 2 ) );
var_dump( $hash->add( 3, 5 ) );
var_dump( $hash->getSize() );
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
bool(true)
bool(true)
int(2)
]]>
    </screen>
   </example>
  </para>
 </refsect1>


</refentry>

