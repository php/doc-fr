<!-- splitted from ./fr/functions/ftp.xml, last change in rev 1.7 -->
<!-- last change to 'ftp-quit' in en/ tree in rev 1.2 -->
 <refentry id="function.ftp-quit">
  <refnamediv>
   <refname>ftp_quit</refname>
   <refpurpose>Ferme une connexion FTP</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>ftp_quit</methodname>
      <methodparam><type>resource</type><parameter>ftp_stream</parameter></methodparam>
     </methodsynopsis>
   <para>
    <function>ftp_quit</function> ferme la connexion
    <parameter>ftp_stream</parameter>.
   </para>
   <para>
     <function>ftp_quit</function> est un alias de 
     <function>ftp_close</function>.
    </para>
  </refsect1>
 </refentry>

