<refentry xml:id="function.ncurses-resetty" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ncurses_resetty</refname>
  <refpurpose>Rétablit le terminal sauvé</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ncurses_resetty</methodname>
