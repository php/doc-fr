<!-- splitted from ./fr/functions/xslt.xml, last change in rev 1.1 -->
<!-- last change to 'xslt-error' in en/ tree in rev 1.3 -->
  <refentry id="function.xslt-error">
   <refnamediv>
    <refname>xslt_error</refname>
    <refpurpose>Retourne le message d'erreur courant</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>mixed</type><methodname>xslt_error</methodname>
       <methodparam choice="opt"><type>resource</type><parameter>xh</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>xslt_errno</function> le message d'erreur courant, pour l'analyseur
      <parameter>xh</parameter>. Si <parameter>xh</parameter> n'est pas
      fourni, le dernier num&eacute;ro d'erreur est retourn&eacute;.
    </para>
   </refsect1>
  </refentry>

