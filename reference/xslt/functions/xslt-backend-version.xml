<refentry xml:id="function.xslt-backend-version" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>xslt_backend_version</refname>
  <refpurpose>Retourne le numéro de version de Sablotron</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>xslt_backend_version</methodname>
   <void/>
  </methodsynopsis>
  <para>
   <function>xslt_backend_version</function> retourne le numéro de version de
   <productname>Sablotron</productname>.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne le numéro de version, ou &false; s'il n'est
   pas disponible.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>xslt_backend_version</function></title>
    <programlisting role="php">
<![CDATA[
<?php

echo xslt_backend_version(); // 0.98 par exemple

?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>xslt_backend_info</function></member>
    <member><function>xslt_backend_name</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

