<refentry xml:id="function.trader-macdfix" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>trader_macdfix</refname>
  <refpurpose>Moyen de déplacement Convergence/Divergence fixé à 12/26</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>trader_macdfix</methodname>
   <methodparam><type>array</type><parameter>real</parameter></methodparam>
   <methodparam choice="opt"><type>integer</type><parameter>signalPeriod</parameter></methodparam>
  </methodsynopsis>
  <para>
   
  </para>
 </refsect1>
 
 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>real</parameter></term>
    <listitem>
     <para>
      &trader.arg.array.of.real;
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>signalPeriod</parameter></term>
    <listitem>
     <para>
      &trader.arg.signal.period;
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne un tableau contenant les données calculées,
   ou &false; si une erreur survient.
  </para>
 </refsect1>
 
 
</refentry>

