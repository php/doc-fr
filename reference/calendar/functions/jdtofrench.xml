<!-- splitted from ./fr/functions/calendar.xml, last change in rev 1.7 -->
<!-- last change to 'jdtofrench' in en/ tree in rev 1.4 -->
 <refentry id="function.jdtofrench">
  <refnamediv>
   <refname>jdtofrench</refname>
   <refpurpose>
    Convertit le nombre de jours du calendrier Julien en date du calendrier
    français républicain
   </refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>string</type><methodname>jdtofrench</methodname>
     <methodparam><type>int</type><parameter>juliandaycount</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>jdtofrench</function> convertit le nombre de jours du calendrier
    julien en date du calendrier français républicain.
   </para>
  </refsect1>
 </refentry>

