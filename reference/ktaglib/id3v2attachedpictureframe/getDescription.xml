<refentry xml:id="id3v2attachedpictureframe.getDescription" xmlns="http://docbook.org/ns/docbook">
  <refnamediv>
    <refname>KTaglib_ID3v2_AttachedPictureFrame::getDescription</refname>
    <refpurpose>Retourne une description de l'image contenu dans la frame spécialisée</refpurpose>
  </refnamediv>
  <refsect1 role="description">
    &reftitle.description;
    <methodsynopsis>
      <modifier>public</modifier>
      <type>string</type>
      <methodname>KTaglib_ID3v2_AttachedPictureFrame::getDescription</methodname>
      <void/>
    </methodsynopsis>
    <para>
     Retourne la description attachée à une frame contenant une image, dans une
     frame ID3v2.x.
    </para>
  </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne une description de l'image.
  </para>
 </refsect1>

</refentry>

