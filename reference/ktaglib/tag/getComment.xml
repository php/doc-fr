<refentry xml:id="tag.getComment" xmlns="http://docbook.org/ns/docbook">
  <refnamediv>
    <refname>KTaglib_Tag::getComment</refname>
    <refpurpose>Retourne le commentaire d'un tag ID3</refpurpose>
  </refnamediv>
  <refsect1 role="description">
    &reftitle.description;
    <methodsynopsis>
      <modifier>public</modifier>
      <type>string</type>
      <methodname>KTaglib_Tag::getComment</methodname>
      <void />
    </methodsynopsis>
    <para>
     Retourne le commentaire d'un tag ID3. Cette méthode est implémentée
     pour les tags ID3v1 et ID3v2.
    </para>
  </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne le commentaire.
  </para>
 </refsect1>

</refentry>

