<?xml version="1.0" encoding="iso-8859-1"?>
<!-- splitted from ./fr/functions/math.xml, last change in rev 1.11 -->
<!-- last change to 'exp' in en/ tree in rev 1.2 -->
 <refentry id="function.exp">
  <refnamediv>
   <refname>exp</refname>
   <refpurpose>Exponentielle</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>float</type><methodname>exp</methodname>
     <methodparam><type>float</type><parameter>arg</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>exp</function> retourne l'exponentielle de
    <parameter>arg</parameter>, c'est-à-dire
    e élevé à la puissance
    <parameter>arg</parameter>.
   </para>
   <para>
    Voir aussi
    <function>pow</function> et 
    <function>log</function>.
   </para>
  </refsect1>
 </refentry>

