<refentry xml:id="xmldiff-dom.merge" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>XMLDiff\DOM::merge</refname>
  <refpurpose>Produit un DOMDocument fusionné</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>DOMDocument</type><methodname>XMLDiff\DOM::merge</methodname>
   <methodparam><type>DOMDocument</type><parameter>src</parameter></methodparam>
   <methodparam><type>DOMDocument</type><parameter>diff</parameter></methodparam>
  </methodsynopsis>
  <para>
   Crée un nouveau DOMDocument basé sur une différence.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>src</parameter></term>
    <listitem>
     <para>
      Objet DOMDocument source.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>diff</parameter></term>
    <listitem>
     <para>
      L'objet DOMDocument contenant les informations de différence.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   DOMDocument fusionné ou NULL.
  </para>
 </refsect1>


</refentry>

