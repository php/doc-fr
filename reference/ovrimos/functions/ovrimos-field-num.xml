<!-- splitted from ./fr/functions/ovrimos.xml, last change in rev 1.7 -->
<!-- last change to 'ovrimos-field-num' in en/ tree in rev 1.1 -->
  <refentry id="function.ovrimos-field-num">
   <refnamediv>
    <refname>ovrimos_field_num</refname>
    <refpurpose>Retourne le numéro de colonne Ovrimos</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>int</type><methodname>ovrimos_field_num</methodname>
      <methodparam><type>int</type><parameter>result_id</parameter></methodparam>
      <methodparam><type>string</type><parameter>field_name</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>ovrimos_field_num</function> retourne le numéro de la colonne
      <parameter>field_name</parameter> (la numérotation commence à 1), dans
      <parameter>result_id</parameter>.
    </para>
   </refsect1>
  </refentry>

