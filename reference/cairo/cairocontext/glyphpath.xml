]]>
    </screen>
   </example>
  </para>
  <para>
   <example>
    <title>Style procédural</title>
    <programlisting role="php">
<![CDATA[
<?php
/* ... */
?>
]]>
     </programlisting>
     &example.outputs.similar;
     <screen>
<![CDATA[
...
]]>
    </screen>
   </example>
  </para>
 </refsect1>

</refentry>

