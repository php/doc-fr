<refentry xml:id="amqpconnection.getport" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>AMQPConnection::getPort</refname>
  <refpurpose>Récupère le port configuré</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>AMQPConnection::getPort</methodname>
   <void />
  </methodsynopsis>
  <para>
   Retourne le port configuré pour cet objet
   <classname>AMQPConnection</classname>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Le port configuré, sous la forme d'un entier.
  </para>
 </refsect1>


</refentry>

