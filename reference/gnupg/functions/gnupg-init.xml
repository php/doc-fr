<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.gnupg-init">
 <refnamediv>
  <refname>gnupg_init</refname>
  <refpurpose>Initialise une connexion</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>resource</type><methodname>gnupg_init</methodname>
   <void />
  </methodsynopsis>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Une ressource de connexion GnuPG, utilisée par les autres fonctions GnuPG.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>gnupg_init</function> (Style procédural)</title>
    <programlisting role="php">
<![CDATA[
<?php
$res = gnupg_init();
?>
]]>
    </programlisting>
   </example>
   <example>
    <title>Exemple avec <function>gnupg_init</function> (Style orienté objet)</title>
    <programlisting role="php">
<![CDATA[
<?php
$gpg = new gnupg();
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
</refentry>

